{
  "name": "commerce-sdk-generator",
  "version": "0.0.1",
  "description": "Commerce SDK Generator, contains the templates to generate the SDK",
  "main": "index.js",
  "scripts": {
    "test": "cross-env TS_NODE_FILES=true mocha --exit --require ts-node/register --colors test/**/*.ts",
    "start": "node dist/index.js",
    "clean": "gulp clean",
    "build": "gulp buildInt && gulp buildSdk",
    "lint": "tsc && eslint \"**/*.{js,ts}\" --quiet --fix",
    "coverage": "nyc npm run test"
  },
  "nyc": {
    "exclude": [
      "**/pkg",
      "./src/core/index.js"
    ],
    "all": true,
    "check-coverage": true,
    "branches": 80,
    "functions": 80,
    "lines": 80,
    "statements": -10
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.9",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "del": "^5.1.0",
    "fetch-mock": "^7.4.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^5.0.1",
    "mocha": "^6.2.0",
    "node-fetch": "^2.6.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "webapi-parser": "^0.3.1"
  },
  "publishConfig": {
    "registry": "https://nexus.soma.salesforce.com/nexus/content/repositories/npmjs-internal/"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {}
}
