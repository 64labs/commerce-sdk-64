{
  "name": "commerce-sdk-generator",
  "version": "0.0.1",
  "description": "Commerce SDK Generator, contains the templates to generate the SDK",
  "main": "index.js",
  "scripts": {
    "pretest": "eslint .",
    "test": "nyc --reporter=text mocha --require @babel/register --exit",
    "start": "babel-node src/index.js"
  },
  "nyc": {
    "exclude": [
      "**/pkg",
      "./src/core/index.js"
    ],
    "all": true,
    "check-coverage": true,
    "branches": 80,
    "functions": 80,
    "lines": 80,
    "statements": -10
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "@istanbuljs/nyc-config-babel": "^2.1.1",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-plugin-rewire": "^1.2.0",
    "chai": "^4.2.0",
    "chai-files": "^1.4.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-xo": "^0.25.0",
    "eslint-plugin-prettier": "^3.1.0",
    "fetch-mock": "^7.3.9",
    "handlebars": "^4.3.1",
    "lint-staged": "^8.1.0",
    "lodash": "^4.17.15",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.2",
    "nyc": "^14.1.1",
    "path": "^0.12.7",
    "prettier": "^1.15.2",
    "promise": "^8.0.3",
    "sinon": "^7.5.0",
    "tmp": "^0.1.0",
    "url-parse": "^1.4.7",
    "webapi-parser": "^0.3.0"
  },
  "publishConfig": {
    "registry": "https://nexus.soma.salesforce.com/nexus/content/repositories/npmjs-internal/"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "node-fetch": "^2.6.0"
  }
}
